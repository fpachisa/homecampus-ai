              "stepByStep": [
                {
                  "step": 1,
                  "explanation": "Isolate the term containing $a$. Move $\\frac{2}{c}$ to the left side.",
                  "working": "$b - \\frac{2}{c} = \\frac{a}{a-1}$",
                  "reasoning": "Preparation for cross-multiplication."
                },
                {
                  "step": 2,
                  "explanation": "Combine the left side into a single fraction.",
                  "working": "$\\frac{bc-2}{c} = \\frac{a}{a-1}$",
                  "reasoning": "Simplifies the subsequent algebra."
                },
                {
                  "step": 3,
                  "explanation": "Cross-multiply to remove fractions.",
                  "working": "$(bc-2)(a-1) = ac$",
                  "reasoning": "Linearizing the equation."
                },
                {
                  "step": 4,
                  "explanation": "Expand the brackets.",
                  "working": "$abc - bc - 2a + 2 = ac$",
                  "reasoning": "Exposing all $a$ terms."
                },
