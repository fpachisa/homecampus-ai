                },
                {
                  "step": 5,
                  "explanation": "Calculate the values and round to 3 decimal places.",
                  "working": "$x_1 = \\frac{-19 + 22.113}{8} \\approx 0.389$, $x_2 = \\frac{-19 - 22.113}{8} \\approx -5.139$",
                  "reasoning": "Final numerical approximation."
                }
              ]
            }
